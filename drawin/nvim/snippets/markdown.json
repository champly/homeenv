{
  "mn": {
    "prefix": "mn",
    "body": ["---<Enter><Enter>"],
    "description": "Dividing line"
  },
  "mb": {
    "prefix": "mb",
    "body": ["****<Space><++><Esc>F*hi"],
    "description": "Font bold"
  },
  "h1 title": {
    "prefix": "h1",
    "body": [
      "# ${1:Title}",
      "",
      "---",
      "",
      "[TOC]",
      "",
      "${2:Next}"
    ]
  },
  "h2 title": {
    "prefix": "h2",
    "body": [
      "## ${1:Title}",
      "",
      "---",
      "",
      "${2:Next}"
    ]
  },
  "h3 title": {
